<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/Inicio"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    android:backgroundTint="#FFFFFF"
    tools:context=".MainActivity"
    tools:ignore="DuplicateIds">

    <View
        android:id="@+id/view2"
        android:layout_width="379dp"
        android:layout_height="731dp"
        android:layout_marginStart="47dp"
        android:layout_marginBottom="282dp"
        android:background="#5DCA61"
        android:rotation="-79"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="101dp"
        android:layout_marginTop="67dp"
        android:layout_marginEnd="102dp"
        android:layout_marginBottom="597dp"
        android:fontFamily="sans-serif-medium"
        android:text="e-Market"
        android:textColor="@color/white"
        android:textSize="50sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="322dp"
        android:layout_height="81dp"
        android:layout_marginStart="36dp"
        android:layout_marginTop="181dp"
        android:layout_marginEnd="36dp"
        android:layout_marginBottom="524dp"
        android:fontFamily="sans-serif-medium"
        android:text="¡La comodidad de comprar
                                     al alcance de tu mano!"
        android:textColor="@color/white"
        android:textSize="26sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/view3"
        android:layout_width="379dp"
        android:layout_height="269dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="350dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="112dp"
        android:background="@drawable/roundstyle"
        android:backgroundTint="#EDEAEA"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.888"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.472"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/textContra"
        android:layout_width="329dp"
        android:layout_height="50dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="450dp"
        android:layout_marginEnd="42dp"
        android:layout_marginBottom="231dp"
        android:background="@drawable/roundstyle"
        android:backgroundTint="#D8D3D3"
        android:hint="Contraseña"
        android:inputType="textPassword"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/btnInicio"
        android:layout_width="339dp"
        android:layout_height="58dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="547dp"
        android:layout_marginEnd="32dp"
        android:layout_marginBottom="126dp"
        android:background="@drawable/roundstyle"
        android:onClick="iniciarSecion"
        android:text="Iniciar Sesión"
        android:textAllCaps="false"
        android:textSize="16sp"
        app:backgroundTint="@color/boton"
        app:cornerRadius="@android:dimen/app_icon_size"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="163dp"
        android:layout_height="29dp"
        android:layout_marginStart="116dp"
        android:layout_marginTop="670dp"
        android:layout_marginEnd="123dp"
        android:layout_marginBottom="76dp"
        android:text="¿No tienes una cuenta?"
        android:textColor="@color/black"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.5" />

    <Button
        android:id="@+id/button3"
        style="?android:attr/borderlessButtonStyle"
        android:layout_width="176dp"
        android:layout_height="52dp"
        android:layout_marginStart="117dp"
        android:layout_marginTop="654dp"
        android:layout_marginEnd="118dp"
        android:layout_marginBottom="25dp"
        android:text="Crea una aquí"
        android:textAllCaps="false"
        android:textColor="#3F51B5"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/textCorreo"
        android:layout_width="331dp"
        android:layout_height="50dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="387dp"
        android:layout_marginEnd="40dp"
        android:layout_marginBottom="294dp"
        android:background="@drawable/roundstyle"
        android:backgroundTint="#D8D3D3"
        android:hint="Usuario"
        android:inputType="textEmailAddress"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

</androidx.constraintlayout.widget.ConstraintLayout>