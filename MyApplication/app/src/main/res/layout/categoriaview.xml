<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:id="@+id/linearLayout"
        android:layout_width="412dp"
        android:layout_height="58dp"
        android:layout_marginTop="672dp"
        android:layout_marginBottom="2dp"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="VisualLintBounds">

        <ImageButton
            android:id="@+id/imageButton"
            android:layout_width="103dp"
            android:layout_height="60dp"
            android:adjustViewBounds="true"
            android:background="@color/white"
            android:contentDescription="@string/app_name"
            android:scaleType="fitXY"
            app:srcCompat="@mipmap/inicio"
            tools:ignore="VisualLintBounds,DuplicateSpeakableTextCheck" />

        <ImageButton
            android:id="@+id/imageButton2"
            android:layout_width="103dp"
            android:layout_height="60dp"
            android:adjustViewBounds="true"
            android:background="@color/white"
            android:contentDescription="@string/app_name"
            android:scaleType="fitXY"
            tools:ignore="VisualLintBounds"
            tools:srcCompat="@mipmap/categoria" />

        <ImageButton
            android:id="@+id/imageButton3"
            android:layout_width="103dp"
            android:layout_height="60dp"
            android:adjustViewBounds="true"
            android:background="@color/white"
            android:contentDescription="@string/app_name"
            android:scaleType="fitXY"
            tools:ignore="VisualLintBounds"
            tools:srcCompat="@mipmap/carrito" />

        <ImageButton
            android:id="@+id/imageButton4"
            android:layout_width="103dp"
            android:layout_height="60dp"
            android:adjustViewBounds="true"
            android:background="@color/white"
            android:contentDescription="@string/app_name"
            android:scaleType="fitXY"
            tools:ignore="VisualLintBounds"
            tools:srcCompat="@mipmap/perfil" />
    </LinearLayout>

    <ListView
        android:id="@+id/listCategoria"
        android:layout_width="408dp"
        android:layout_height="589dp"
        android:layout_marginStart="2dp"
        android:layout_marginTop="81dp"
        android:layout_marginEnd="1dp"
        android:layout_marginBottom="61dp"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>